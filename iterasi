import matplotlib.pyplot as plt
import numpy
import math

a = float(input("Masukkan a: "))
b = float(input("Masukkan b: "))
c = float(input("Masukkan c: "))

def func(x):
    return a*x**2 + b*x + c

def gx(x):
    return math.sqrt((-1 * (b*x + c)) / a)

tebak = float(input("masukkan tebakan: "))
err = float(input("Masukkan error: "))
i = 1
e = 100

while (e>err):
    yx = gx(tebak)
    e = abs(tebak-yx)
    if abs(e) > err:
            tebak = yx
print(tebak)
